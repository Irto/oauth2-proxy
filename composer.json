    "require": {
        "react/http": "0.*",
        "react/http-client": "0.*",
        "illuminate/container": "5.*",
        "illuminate/pipeline": "5.*",
        "illuminate/support": "5.*",
        "illuminate/session": "5.*",
        "illuminate/filesystem": "5.*",
        "symfony/http-foundation": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*"
    },
    "autoload": {
        "psr-4": {
            "Irto\\OAuth2Proxy\\": "src/"
        }
    }
}